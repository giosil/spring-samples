<settings>
  <servers>
    <server>
      <username>devops-mvn-deploy</username>
      <password>xxx</password>
      <id>central</id>
    </server>
    <server>
      <username>devops-mvn-deploy</username>
      <password>xxx</password>
      <id>snapshots</id>
    </server>
    <server>
      <username>devops-mvn-deploy</username>
      <password>xxx</password>
      <id>releases</id>
    </server>
  </servers>
  <mirrors>
    <mirror>
      <id>dew-repo</id>
      <name>Nexus repo</name>
      <url>https://nexus.dew.org/repository/maven-public</url>
      <mirrorOf>*</mirrorOf>
    </mirror>
  </mirrors>
  <profiles>
    <profile>
      <repositories>
        <repository>
          <id>central</id>
          <name>maven-central</name>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <url>https://nexus.dew.org/repository/maven-public</url>
        </repository>
        <repository>
          <id>public</id>
          <name>maven-public</name>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <url>https://nexus.dew.org/repository/maven-public</url>
        </repository>
        <repository>
          <id>snapshots</id>
          <name>snapshot</name>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
          <url>https://nexus.dew.org/repository/maven-snapshots/</url>
        </repository>
        <repository>
          <id>releases</id>
          <name>releases</name>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <url>https://nexus.dew.org/repository/maven-releases/</url>
        </repository>
      </repositories>
      <properties>
        <repository.url>https://nexus.dew.org/repository/</repository.url>
        <altSnapshotDeploymentRepository>snapshots::default::https://nexus.dew.org/repository/maven-snapshots/</altSnapshotDeploymentRepository>
        <altReleaseDeploymentRepository>releases::default::https://nexus.dew.org/repository/maven-releases/</altReleaseDeploymentRepository>
        <altDeploymentRepository>snapshots::default::https://nexus.dew.org/repository/maven-snapshots/</altDeploymentRepository>
      </properties>
      <id>dew</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>dew</activeProfile>
  </activeProfiles>
</settings>
